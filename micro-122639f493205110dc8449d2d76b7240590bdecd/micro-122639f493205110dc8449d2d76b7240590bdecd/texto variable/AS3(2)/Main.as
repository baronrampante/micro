package {	import flash.display.MovieClip;	import flash.events.MouseEvent;	public class Main extends MovieClip	{		var NumeroUno:Number;		var NumeroDos:Number;		var Resultado:Number;		public function Main()		{			Suma_btn.addEventListener(MouseEvent.CLICK, fsuma);			Resultado_btn.addEventListener(MouseEvent.CLICK, fresultado);		}		public function fsuma(mc:MouseEvent):void		{			NumeroUno = Number(Numeros_txt.text);			Numeros_txt.text = "";		}		public function fresultado(mc:MouseEvent):void		{			NumeroDos = Number(Numeros_txt.text);			Resultado = NumeroUno + NumeroDos;			Numeros_txt.text = String(Resultado);		}	}}